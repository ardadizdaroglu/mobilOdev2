<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.MobilOdev2">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.MobilOdev2.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.MobilOdev2.SettingsActivity" />
        <activity android:name="com.example.MobilOdev2.NotificationActivity" />
        <activity android:name="com.example.MobilOdev2.EklemeActivity" />
        <activity android:name="com.example.MobilOdev2.GuncellemeSilmeActivity" />
        <activity android:name="com.example.MobilOdev2.MainActivity">
        </activity>

        <receiver
            android:name="com.example.MobilOdev2.AlarmReceiver"
            android:process=":remote" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>